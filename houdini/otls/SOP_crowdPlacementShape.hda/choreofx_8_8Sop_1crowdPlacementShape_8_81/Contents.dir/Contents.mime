vex_precision	[ 0	locks=0	autoscope=0 ]	(	auto	)
parameters	[ 0	locks=0	autoscope=0 ]	(	1	1	1	)
upVector	[ 0	locks=0	autoscope=0 ]	(	[ upVectorx	0 ] 	[ upVectory	0 ] 	[ upVectorz	0 ] 	)
