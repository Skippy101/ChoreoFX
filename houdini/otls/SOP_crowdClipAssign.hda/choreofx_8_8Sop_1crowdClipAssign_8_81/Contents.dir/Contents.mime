--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/randomize_clip_patterns.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	[ group	__selectedPoints ] 	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"if (chi(\"../overrideExisting\") == 0){
    if (s@clipName != \"random\" && s@clipName != \"\"){
        return;
    }
}

int num_patterns = chi(\"../numClipPatterns\");
float seed = chf(\"../randomClipSeed\");

string clips[];
float weights[];

// If we did not copy agent prims to points, 
// we can lookup clips by agentname from an external source
int agentprim = @primnum;
int inputidx = 0;

if (chi(\"useExternalAgents\")){
//if (agentprim < 0){
    agentprim = findattribval(1, \"point\", \"agentname\", s@agentname, 0);
    inputidx = 1;
    
    if (agentprim < 0){
        warning(\"No agent prim found to lookup clips from for '%s'\", s@agentname);
        return;
    }
}

string clip_catalog[] = agentclipcatalog(inputidx, agentprim);

for (int i = 1; i <= num_patterns; ++i)
{
    string pattern = chs(sprintf(\"../clipPattern_%d\", i));
    float weight = chf(sprintf(\"../clipWeight_%d\", i));

    string matching_clips[];

    foreach (string clipname; clip_catalog)
    {
        //if (clipname == \"rest\" || clipname == \"Tpose\")
        //    continue;
            
        if (match(pattern, clipname))
            append(matching_clips, clipname);
    }

    int num_clips = len(matching_clips);
    if (num_clips > 0)
    {
        append(clips, matching_clips);

        weight /= num_clips;
        for (int j = 0; j < num_clips; ++j)
            append(weights, weight);
    }
    else{
        warning(\"No valid clips were found for '%s'\", s@agentname);
    }
}

int ptid = i@`chs(\"seedAttrib\")`;
float u = rand(ptid + 231 * seed);
s@clipName = clips[sample_discrete(weights, u)];
//s[]@clips = clips;
"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
vex_strict	[ 0	locks=0	autoscope=0 ]	(	"off"	)
autobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
bindings	[ 0	locks=0	autoscope=0 ]	(	0	)
groupautobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
groupbindings	[ 0	locks=0	autoscope=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0	autoscope=0 ]	(	.	)
vex_outputmask	[ 0	locks=0	autoscope=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0	autoscope=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0	autoscope=0 ]	(	id	)
vex_inplace	[ 0	locks=0	autoscope=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0	autoscope=0 ]	(	""	)
vex_precision	[ 0	locks=0	autoscope=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
seedAttrib	[ 0	locks=0	autoscope=0 ]	(	[ seedAttrib	agent_id ] 	)
useExternalAgents	[ 0	locks=0 ]	(	[ useExternalAgents	1 ] 	)
}

