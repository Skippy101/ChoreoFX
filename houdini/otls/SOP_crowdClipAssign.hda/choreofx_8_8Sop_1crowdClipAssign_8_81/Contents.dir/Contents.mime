float enablelayer = chf(sprintf(\"../manualEnable_%d\", i));
string clip = chs(sprintf(\"../manualClipName_%d\", i));

if (enablelayer == 1)
{
    if (chi(\"verify\") == 1){
        int found = find(agentclipcatalog(0, @primnum), clip);
        if (found > -1){
            s@clipName = clip;
        }
        else
        {
            s@clipName = \"rest\";
            warning(\"Clip '%s' does not match any clips for '%s'\", clip, s@agentname);
        }
    }
    else{
        s@clipName = clip;    
