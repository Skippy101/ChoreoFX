Content-Disposition: attachment; filename="hdaroot/randomize_prop_patterns.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	[ group	"" ] 	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"points"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"float seed = chf(\"../randomPropSeed\");
int num_patterns = chi(\"../numPropPatterns\");
if (num_patterns > 0)
{
    string allprops[] = uniquevals(1, \"point\", \"propName\");
    
    string props[];
    float weights[];
    
    for (int i = 1; i <= num_patterns; ++i)
    {
        string pattern = chs(sprintf(\"../propPattern_%d\", i));
        float weight = chf(sprintf(\"../propWeight_%d\", i));
        
        int num_matches = 0;
        foreach (string propname; allprops)
        {
            if (match(pattern, propname)){
                append(props, propname);
                num_matches++;
            }
        }
        
        if (num_matches > 0)
        {
            weight /= num_matches;
            for (int j = 0; j < num_matches; ++j)
                append(weights, weight);
        }
        else
        {
            //warning(\"Pattern '%s' does not match any layers for '%s'\", pattern, s@agentname);
        }
    
    }
    
    
        int seedid = i@`chs(\"seedAttrib\")`;
        float u = rand(set(seedid, seed));
        string prop_name = props[sample_discrete(weights, u)];
        
        if (len(prop_name)>0){
            s@propName = prop_name;
    }
}"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
