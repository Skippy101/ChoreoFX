string propJointName = s@propJointName;

if (chi(\"../useOverrideJoint\")){
    propJointName = chs(\"../overrideJoint\");
}

int propTransId = agentrigfind(1, i@ptid, propJointName);

if (propTransId < 0){
    if (chi(\"../useFallbackJointList\"))
    {
        string jointList = chs(\"../fallbackJoints\");
        foreach (string jointname; split(jointList))
        {
            int foundid = agentrigfind(1, i@ptid, jointname);
            if (foundid > -1){
                propTransId = foundid; 
            }
        }
        
        if (propTransId < 0){
            warning(\"No matching joint found for prop '%s' on agent %d\", s@propName, i@ptid);    
            removepoint(0, @ptnum);
        }
    }
    else{
        removepoint(0, @ptnum);
    }
}
i@atest = propTransId;
matrix agentPackedFullTrans = primintrinsic(1, \"packedfulltransform\", i@ptid);

matrix offsetXform = 4@offsetXform;
matrix newXform = ident();
matrix jointXform = agentworldtransform(1, i@ptid, propTransId);
newXform *= polardecomp(matrix3(jointXform));
vector pos = {0,0,0} * jointXform;
translate(newXform, pos);
//vector jointScale = cracktransform(0, 0, 2, {0,0,0}, invert(jointXform));
//scale(offsetXform, jointScale);
jointXform = newXform;
jointXform *= agentPackedFullTrans;

if (applyScale == 0){
    vector agentScale =  {1,1,1} * point(1, \"pscale\", i@ptid);
    
    scale(offsetXform, 1/agentScale);
}

4@propXform = offsetXform * jointXform;
@P = {0,0,0};"	)
