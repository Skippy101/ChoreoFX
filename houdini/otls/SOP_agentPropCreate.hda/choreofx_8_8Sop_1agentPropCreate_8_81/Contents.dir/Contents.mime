{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	""	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"int propTransId = i@propJointId;

int applyScale = chi(\"../applyAgentScale\");

matrix agentPackedFullTrans = primintrinsic(1, \"packedfulltransform\", 0);

matrix offsetXform = 4@offsetXform;

matrix jointXform = agentworldtransform(1, 0, propTransId);
matrix newXform = ident();
newXform *= polardecomp(matrix3(jointXform));
vector pos = {0,0,0} * jointXform;
translate(newXform, pos);
newXform *= agentPackedFullTrans;

if (applyScale == 0){
    vector agentScale =  {1,1,1} * point(1, \"pscale\", 0);
    
    scale(offsetXform, 1/agentScale);
}

4@propXform = offsetXform * newXform;
"	)
