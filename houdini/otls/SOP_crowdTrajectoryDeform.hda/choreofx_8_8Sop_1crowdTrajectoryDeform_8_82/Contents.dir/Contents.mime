Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	[ group	_pathdeform ] 	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	[ grouptype	4 ] 	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"int primid =  prim(0, \"deformprimid\", @primnum);

float minu = prim(2, \"min_u\", primid);
float maxu = prim(2, \"max_u\", primid);
float percent = fit(@u, minu, maxu, 0, 1); 
float percent2 = percent;
if (maxu > 1){
    percent = fit(@u, minu, maxu, 0, maxu);   
    percent2 = percent/(maxu);
}
//@percent = percent;
//@minu = minu;
//@maxu = maxu;

matrix3 curveTM = primuv(2, \"TM\", primid, percent2);
vector curvePos = primuv(2, \"P\", primid, percent2);

matrix3 curveTM_rest = primuv(1,\"TM\", primid, percent);
vector curvePos_rest = primuv(1,\"P\", primid, percent);

vector offsetVec = @P - curvePos_rest;
v@offsetVec = offsetVec * invert(curveTM_rest);
@orient = qmultiply(@orient, quaternion(invert(curveTM_rest)));

@P = curvePos + v@offsetVec * curveTM;
@orient = qmultiply(@orient, quaternion(curveTM));
    
"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
vex_strict	[ 0	locks=0	autoscope=0 ]	(	"off"	)
autobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
bindings	[ 0	locks=0	autoscope=0 ]	(	0	)
groupautobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
groupbindings	[ 0	locks=0	autoscope=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0	autoscope=0 ]	(	.	)
vex_outputmask	[ 0	locks=0	autoscope=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0	autoscope=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0	autoscope=0 ]	(	id	)
vex_inplace	[ 0	locks=0	autoscope=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0	autoscope=0 ]	(	""	)
vex_precision	[ 0	locks=0	autoscope=0 ]	(	auto	)
folder1	[ 0	locks=0	autoscope=0 ]	(	0	0	)
attribute	[ 0	locks=0	autoscope=0 ]	(	[ attribute	primid ] 	)
}

