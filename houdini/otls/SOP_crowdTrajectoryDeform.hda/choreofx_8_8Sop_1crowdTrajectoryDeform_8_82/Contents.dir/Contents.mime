--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/deform_pos_lerp.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	""	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"// figure out if the points are within the handle range
float primu = @primu;
//int prim = pointprims(0, @ptnum)[0];
int primid = -1;
string attribname = chs(\"../primIdAttrib\");
if (hasprimattrib(0, attribname)){
    primid = prim(0, attribname, @primnum);
}
else if (haspointattrib(0, attribname)){
    primid = point(0, attribname, primpoint(0, @primnum, 0));
}
else{
    primid = @primnum;
}

int prim = findattribval(1, \"prim\", \"primid\", primid, 0);

if (prim < 0){
    return;
}
int primpoints[] = primpoints(1, prim);
@group_deformed = 1;

// If only has one handle then just move with it
if (len(primpoints) == 1){
    vector offset = point(1, \"posOffset\", primpoints[0]);
    @P += offset;
    return;
}

float min_u = prim(1, \"min_u\", prim);
float max_u = prim(1, \"max_u\", prim);

// figure out between what handles this point is
float handles [] = prim(1, \"handlesu\", prim);
int handlecount = len(handles);
int prev_handle = -1;
int next_handle = -1;

// Adjust points before first handle and after last handle
if(primu < handles[0]){
    prev_handle = 0;
    next_handle = 1;
}
else if(primu > handles[-1]){
    prev_handle = handlecount-2;
    next_handle = handlecount-1;
}
else
{
    for(int handle_idx = 0; handle_idx < handlecount; handle_idx++){
        float test_prev_handle_u = handles[handle_idx];
        float test_next_handle_u = handles[handle_idx + 1];
        
        if(test_prev_handle_u <= primu && test_next_handle_u >= primu){
            prev_handle = handle_idx;
            next_handle = handle_idx+1;
            break;
        }
    }
}
// error if prev and next are still -1
if(prev_handle == -1 || next_handle == -1){
    error(sprintf(\"Handles not found for point %d\", @ptnum));
}

@prev_handle = prev_handle;
@next_handle = next_handle;

// first construct the line between the two handles in the current trajectory
vector cur_prev_handle_pos = point(1, \"restP\", primpoints[prev_handle]);
vector cur_next_handle_pos = point(1, \"restP\", primpoints[next_handle]);
//vector cur_line [] = array(cur_prev_handle_pos, cur_next_handle_pos);

//construct the line between the two handles in the target trajectory
vector tar_prev_handle_pos = point(1, \"P\", primpoints[prev_handle]);
vector tar_next_handle_pos = point(1, \"P\", primpoints[next_handle]);
//vector tar_line [] = array(tar_prev_handle_pos, tar_next_handle_pos);


//create a value between 0 curr tangent, and 1 next tangent.
//float lerp_amount = fit(@ptnum, prev_handle, next_handle, 0, 1);
float lerp_amount = fit(primu, handles[prev_handle], handles[next_handle], 0, 1);
f@lerp = lerp_amount;

// update primu with constrained u for accuracy of pystate selections. TODO
//@primu = lerp(handles[prev_handle], handles[next_handle], lerp_amount);
//@primu = ((handles[next_handle] - handles[prev_handle]) * lerp_amount) + handles[prev_handle];
//lerp_amount =  smooth(0, 1, lerp_amount, 1);

//figure out the current position based on the lerp value on the current line between tangents
vector cur_line_pos = lerp(cur_prev_handle_pos, cur_next_handle_pos, lerp_amount);
// firgure out the target position based on the lerp valued on the target line between tangents
vector tar_line_pos = lerp(tar_prev_handle_pos, tar_next_handle_pos, lerp_amount);

//add the curve's offset to the target pos

//get the offset and store it as a vector
vector offset = tar_line_pos  - cur_line_pos ;
@P += offset;

"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
vex_strict	[ 0	locks=0	autoscope=0 ]	(	"off"	)
autobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
bindings	[ 0	locks=0	autoscope=0 ]	(	0	)
groupautobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
groupbindings	[ 0	locks=0	autoscope=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0	autoscope=0 ]	(	.	)
vex_outputmask	[ 0	locks=0	autoscope=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0	autoscope=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0	autoscope=0 ]	(	id	)
vex_inplace	[ 0	locks=0	autoscope=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0	autoscope=0 ]	(	""	)
