scaleRamp1pos	[ 0	locks=0	autoscope=0 ]	(	[ scaleRamp1pos	0 ] 	)
scaleRamp1value	[ 0	locks=0	autoscope=0 ]	(	[ scaleRamp1value	1 ] 	)
