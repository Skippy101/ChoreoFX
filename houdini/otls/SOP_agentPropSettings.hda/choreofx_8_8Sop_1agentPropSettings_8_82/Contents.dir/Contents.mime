Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	""	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"matrix agentPackedFullTrans = primintrinsic(1, \"packedfulltransform\", 0);

matrix offsetXform = 4@offsetXform;
matrix agentJointXform = agentworldtransform(1, 0, i@agentJointId);
matrix propJointXform = agentworldtransform(0, @primnum, i@propJointId);

matrix propXform = ident();
propXform *= polardecomp(matrix3(agentJointXform));
vector pos = {0,0,0} * agentJointXform;
translate(propXform, pos);

propXform *= (invert(propJointXform) * agentPackedFullTrans);

if (chi(\"../applyAgentScale\") == 0){
    vector agentScale =  {1,1,1} * point(1, \"pscale\", 0);
    
    scale(offsetXform, 1/agentScale);
}

4@propXform = propXform;
4@propXform = offsetXform * propXform;
setagentlocaltransform(0, @primnum, 4@propXform, i@propJointId);
"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
