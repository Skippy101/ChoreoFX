Content-Disposition: attachment; filename="hdaroot/mirrorMap.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	[ group	"" ] 	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"int mirrorMap[];

string jointNames[] = agenttransformnames(0, i@primnum);

int findNameInJoints(string names[]; string name){

    int found = find(names, name);
    return (found > -1);
}

for (int joint_idx = 0; joint_idx < len(jointNames); joint_idx++)
{
    //Save Pose and align to rest pose
    if (agentrigparent(0, i@primnum, joint_idx) == -1)
    {
        mirrorMap[joint_idx] = joint_idx;
        continue;
    }
    
    // JOINT NAME MAPPINGS
    string jointName = jointNames[joint_idx];
    string mirrorName;
    
    int L_to_R = 0;
    int R_to_L = 0;
    
    for( int mapping = 0; mapping < chi(\"mappings\"); ++mapping )
    {
        string leftPrefix = chs(sprintf(\"leftPrefix%d\", mapping+1));
        string rightPrefix = chs(sprintf(\"rightPrefix%d\", mapping+1));
        int useAsPrefix = chi(sprintf(\"useAsPrefix%d\", mapping+1));
        
        if (!useAsPrefix)
        {
            mirrorName = re_replace(leftPrefix, rightPrefix, jointName);
            if (mirrorName != jointNames[joint_idx])
            {
                L_to_R = 1;
            }
        }
        else
        {
            if (startswith(jointName, leftPrefix))
            {
                
                mirrorName = rightPrefix + jointName[strlen(leftPrefix):];
                L_to_R = 1;
            }
        }

        if (!L_to_R)
        {
            if (!useAsPrefix)
            {
                mirrorName = re_replace(rightPrefix, leftPrefix, jointName);
                if (mirrorName != jointNames[joint_idx])
                {
                    R_to_L = 1;
                }
            }
            else
            {
                if (startswith(jointName, rightPrefix))
                {
                    R_to_L = 1;
                    mirrorName = leftPrefix + jointName[strlen(rightPrefix):];
                }
            }
            
        }
            
        if (findNameInJoints(jointNames, mirrorName) == 0){ 
            R_to_L = 0;
            L_to_R = 0;
        }
        
        // If we found a match we can stop searching
        if (L_to_R || R_to_L)
        {
            break;
        }
    }
    

    int opp_joint_idx = agentrigfind(0, i@primnum, mirrorName);
    if (opp_joint_idx == -1)
    {
        mirrorMap[joint_idx] = joint_idx;
        continue;
    }
        
    mirrorMap[joint_idx] = opp_joint_idx;
    mirrorMap[opp_joint_idx] = joint_idx;

}
i[]@mirrorMap = mirrorMap;


















"	)
