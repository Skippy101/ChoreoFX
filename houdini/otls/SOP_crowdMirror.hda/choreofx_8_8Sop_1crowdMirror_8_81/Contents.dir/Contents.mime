snippet	[ 0	locks=0	autoscope=0 ]	(	"//int mirrorMap[] = point(0, \"mirrorMap\", @primnum);

// All joint world transforms of rest clip
matrix restXforms[] = agentclipsampleworld(0, i@primnum, chs(\"restClip\"), 0.0);
int jointCount = len(restXforms);

int flippedMap[];
resize(flippedMap, jointCount);
int processed[];
resize(processed, jointCount);

for (int joint_idx = 0; joint_idx < jointCount; joint_idx++)
{
    // Dirty way to ensure we only process a pair of joints once.
    if (processed[joint_idx] == 1)
        continue;

    // Get index of mirrored joint if it was one (else self idx)
    int opp_idx = i[]@mirrorMap[joint_idx]; 
    
    // Joints that are scale-mirrored in the RestRig will need orient compensation later
    vector scale = cracktransform(0,0,2,{0,0,0}, restXforms[joint_idx]); 
    vector scale_opp = cracktransform(0,0,2,{0,0,0}, restXforms[opp_idx]); 
    
    if (sign(scale) != sign(scale_opp)){
        flippedMap[joint_idx] = 1;
        flippedMap[opp_idx] = 1;
    }
    
    processed[joint_idx] = 1;
    processed[opp_idx] = 1;
    
    // This version does nothing with the orient difference. Might be used later
    
    //vector4 rot = quaternion(matrix3(restXforms[joint_idx]));
    //vector4 rot_opp = quaternion(matrix3(restXforms[opp_idx]));
    //vector4 orient_restAlignOpp = qmultiply(orient_opp, qinvert(orient));
    
}

i[]@flippedMap = flippedMap;





