Content-Disposition: attachment; filename="hdaroot/mirror_plane.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	[ group	"" ] 	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"
// reflection Plane
string mirrorPlane = chs(\"reflectionPlane\");
// plane Origin
vector pOrigin = chv(\"planeOrigin\");
// plane Normal
vector pN = set(0,0,1);

if ( mirrorPlane == \"x\" )
{
    pN = set(1,0,0);
}
else
if ( mirrorPlane == \"custom\" )
{
    matrix3 rot = ident();
    rotate(rot, radians(ch(\"angle\")), {0,1,0});
    pN = set(1,0,0)*  rot;
}
else
if ( mirrorPlane == \"agent\" )
{
    matrix agentXform = qconvert(p@orient);
    pN = vector(set(0,0,1,0)*agentXform);
    
    matrix3 rot = ident();
    rotate(rot, radians(ch(\"angle\")), {0,1,0});
    pN = pN*  rot;
    
        
    pOrigin = v@P + pOrigin * agentXform * rot;
}
pN = normalize(pN);

// reflection Matrix
// from https://en.wikipedia.org/wiki/Transformation_matrix#Reflection_2
matrix mirrorXform = ident();

float d = -dot(pOrigin, pN);
mirrorXform.xx = 1 - 2 * pN.x * pN.x;
mirrorXform.xy = - 2 * pN.x * pN.y;
mirrorXform.xz = - 2 * pN.x * pN.z;
mirrorXform.yx = mirrorXform.xy;
mirrorXform.yy = 1 - 2 * pN.y * pN.y;
mirrorXform.yz = - 2 * pN.y * pN.z;
mirrorXform.zx = mirrorXform.xz;
mirrorXform.zy = mirrorXform.yz;
mirrorXform.zz = 1 - 2 * pN.z * pN.z;
mirrorXform.wx = - 2 * pN.x * d;
mirrorXform.wy = - 2 * pN.y * d;
mirrorXform.wz = - 2 * pN.z * d;

4@mirrorXform = mirrorXform;
"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
vex_strict	[ 0	locks=0	autoscope=0 ]	(	"off"	)
autobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
bindings	[ 0	locks=0	autoscope=0 ]	(	0	)
groupautobind	[ 0	locks=0	autoscope=0 ]	(	"on"	)
groupbindings	[ 0	locks=0	autoscope=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0	autoscope=0 ]	(	.	)
