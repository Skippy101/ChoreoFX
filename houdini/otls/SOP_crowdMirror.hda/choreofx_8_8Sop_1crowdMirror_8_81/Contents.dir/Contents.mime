int oppId = mirrorMap[@ptnum]; 
i@oppId = oppId;
vector4 orient = @orient;
vector4 orient_opp = point(0, \"orient\", oppId);
p@orient_restOrient = @orient;

p@orient_restAlignOpp = qmultiply(orient_opp, qinvert(orient));

// Just to clean up precision errors on joints with same orient:
if (qdistance(orient, p@orient_restAlignOpp) < ch(\"threshold\")){
    p@orient_restAlignOpp = set(0,0,0,1);
}
/*
i@flipped = 0;
vector4 fliprot = qmultiply(orient, quaternion(radians(180), {1,0,0}));
if (dot(qrotate(orient, {0,0,1}), qrotate(orient_opp, {0,0,1})) > ch(\"threshold\")){
    i@flipped = 1;
}*/

i@flipped = 0;
if (sign(v@scale) != sign(point(0, \"scale\", oppId))){
    i@flipped = 1;    
}

//vector pos_opp = point(0, \"P\", oppId);
