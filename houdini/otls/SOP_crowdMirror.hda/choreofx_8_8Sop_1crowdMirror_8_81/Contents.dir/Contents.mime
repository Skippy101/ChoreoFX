vex_precision	[ 0	locks=0	autoscope=0 ]	(	auto	)
folder1	[ 0	locks=0	autoscope=0 ]	(	0	0	)
angle	[ 0	locks=0	autoscope=0 ]	(	[ angle	90 ] 	)
planeOrigin	[ 0	locks=0	autoscope=0 ]	(	[ planeOriginx	0 ] 	[ planeOriginy	0 ] 	[ planeOriginz	0 ] 	)
