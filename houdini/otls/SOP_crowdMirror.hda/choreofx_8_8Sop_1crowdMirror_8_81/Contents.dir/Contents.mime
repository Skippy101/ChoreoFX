{
    if (!haspointattrib(0, \"up\"))
    {
        @up = {0,1,0};    
    }
    
    if (haspointattrib(0, \"v\"))
    {
        @orient = quaternion(maketransform(normalize(@v), @up));
        @N = normalize(@v);
    }
    else if (haspointattrib(0, \"N\"))
    {
        @orient = quaternion(maketransform(@N, @up)); 
    }
    else
    {
        @N = {0,0,1};
        @orient = quaternion(maketransform(@N, @up));
        
