Content-Disposition: attachment; filename="hdaroot/mirror_agent_xforms.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 8	locks=0	autoscope=0 ]	(	[ group	"" ] 	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"int mirrorMap[] = point(1, \"mirrorMap\", @primnum);
int flippedMap[] = point(1, \"flippedMap\", @primnum);

matrix xforms[] = agentworldtransforms(0, i@primnum);
matrix mirrorXforms[] = xforms;

// For each xform
for (int joint_idx = 0; joint_idx < len(xforms); joint_idx++)
{
    matrix xform_opp = xforms[mirrorMap[joint_idx]];

    // Extract components of mirror opposite
    vector pos = set(xform_opp.wx, xform_opp.wy, xform_opp.wz);
    vector scale = cracktransform(0,0,2,{0,0,0}, xform_opp);
    vector4 rot = quaternion(matrix3(xform_opp));

    // flip rot over x axis
    rot.y *= -1;
    rot.z *= -1;
    
    // flip pos over x axis
    pos.x *= -1;

    // Rotate 'flippers' 180 around their local x axis
    if (flippedMap[joint_idx] == 1){
        rot = qmultiply(rot, {1,0,0,0});
        scale *= -1;
        //rot = qmultiply(rot, quaternion(radians(180), {1,0,0})); //same
    }
    
    // Build mirrored transform
    matrix xform = qconvert(rot);
    scale(xform, scale);
    translate(xform, pos);

    mirrorXforms[joint_idx] = xform;
}
setagentworldtransforms(0, i@primnum, mirrorXforms);
"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
