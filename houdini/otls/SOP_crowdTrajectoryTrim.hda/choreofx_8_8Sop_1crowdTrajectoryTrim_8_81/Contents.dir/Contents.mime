--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/extend_primitives_be_deleting.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	[ group	* ] 	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int prim_pts [] = primpoints(0, @primnum);
int num_prim_pts = len(prim_pts);
int new_traj_start = i@new_traj_start;
int new_traj_end = i@new_traj_end;
//we will create a new prim out of this array
int new_prim_pts[] = prim_pts;
// extend if out of range

// this is either positive (extend the end) or negative (extend the beginning)
int num_pts_to_prepend;
int num_pts_to_extend;
int start_global_frame = point(0, \"global_frame\", prim_pts[0]);
if(start_global_frame > new_traj_start){
    num_pts_to_prepend = start_global_frame - new_traj_start;
}
int end_global_frame = point(0, \"global_frame\", prim_pts[-1]);
//i@end_global_frame = end_global_frame;

if(end_global_frame < new_traj_end){
    num_pts_to_extend = new_traj_end - end_global_frame ;
}
if(num_pts_to_extend > 0 || num_pts_to_prepend > 0){

vector start_pos = point(0, \"P\", prim_pts[0]);
vector4 start_orient = point(0, \"orient\", prim_pts[0]);
matrix start_xform = qconvert(start_orient, start_pos);

vector end_pos = point(0, \"P\", prim_pts[-1]);
vector4 end_orient = point(0, \"orient\", prim_pts[-1]);
matrix end_xform = qconvert(end_orient, end_pos);


// preped points
for(int pt_idx = 0; pt_idx < num_pts_to_prepend; pt_idx ++){
    int ref_ptnum = prim_pts[-1] - (pt_idx % num_prim_pts);
    vector ref_P = point(0, \"P\", ref_ptnum);
    vector ref_orient = point(0, \"orient\", ref_ptnum);
    int ref_global_frame = point(0, \"global_frame\", ref_ptnum);
    matrix ref_xform = qconvert(ref_orient, ref_P);
    
    matrix difference = start_xform * invert(end_xform);
    
    int num_loops = floor(pt_idx / num_prim_pts);
    for(int loop_idx = 0; loop_idx < num_loops; loop_idx++){
       difference *= difference;
    }
    
    vector new_P = ref_P * difference;
    vector4 new_orient = qmultiply(ref_orient, quaternion(matrix3(difference)));
    
    int new_pt = addpoint(0, new_P);
    setpointattrib(0, \"orient\", new_pt, new_orient);
    setpointattrib(0, \"global_frame\", new_pt, start_global_frame - pt_idx);
    setpointattrib(0, \"ref_ptnum\", new_pt, ref_ptnum);
    setpointgroup(0, \"copy_attribs\", new_pt, 1);
    insert(new_prim_pts, 0, new_pt);
}

//i@num_pts_to_extend = num_pts_to_extend;
// extend points
// start at 1 because trajectory is intentionally 1 frame too long
// this is to allow for looping correctly.
for(int pt_idx = 1; pt_idx < num_pts_to_extend; pt_idx ++){
    int ref_ptnum = prim_pts[0] +  (pt_idx % num_prim_pts) ;
    vector ref_P = point(0, \"P\", ref_ptnum);
    vector ref_orient = point(0, \"orient\", ref_ptnum);
    int ref_global_frame = point(0, \"global_frame\", ref_ptnum);
    matrix ref_xform = qconvert(ref_orient, ref_P);
    
    matrix difference = invert(start_xform) * end_xform;
    matrix mult_diff = difference;
    int num_loops = floor(pt_idx / num_prim_pts);
    for(int loop_idx = 0; loop_idx < num_loops; loop_idx++){
       difference *= mult_diff;
    }
    
    vector new_P = ref_P * difference;
    vector4 new_orient = qmultiply(ref_orient, quaternion(matrix3(difference)));
    
    int new_pt = addpoint(0, new_P);
    setpointattrib(0, \"orient\", new_pt, new_orient);
    setpointattrib(0, \"global_frame\", new_pt, end_global_frame + pt_idx);
    setpointattrib(0, \"ref_ptnum\", new_pt, ref_ptnum);
    setpointattrib(0, \"ref_primnum\", new_pt, i@ref_primnum);
    setpointgroup(0, \"copy_attribs\", new_pt, 1);
    append(new_prim_pts, new_pt);
    
    
}


addprim(0, \"polyline\", new_prim_pts);
removeprim(0, @primnum, 0);
}
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
