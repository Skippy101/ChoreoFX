snippet	[ 0	locks=0 ]	(	"
if(chi(\"../put_first_to_start\")){
    // get first point in trimmed points
    int start_pt =prim(0, \"start_ptnum\", @primnum);

    vector4 start_orient = point(0, \"orient\", start_pt);
    vector start_pos = point(0, \"P\", start_pt);
    
    matrix start_xform = qconvert(start_orient);
    translate(start_xform, start_pos);
    start_xform = invert(start_xform);
    
    vector4 orig_start_orient = point(1, \"orient\", 0);
    vector orig_start_pos = point(1, \"P\", 0);
    matrix orig_start_xform = qconvert(orig_start_orient);
    translate(orig_start_xform, orig_start_pos);
    
    vector P = @P;
    
    P *= start_xform;
    P *= orig_start_xform;
    
    @P = P;
    
    p@orient = qmultiply(quaternion(matrix3(start_xform)), p@orient);
    p@orient = qmultiply(quaternion(matrix3(orig_start_xform)), p@orient);
    
}






