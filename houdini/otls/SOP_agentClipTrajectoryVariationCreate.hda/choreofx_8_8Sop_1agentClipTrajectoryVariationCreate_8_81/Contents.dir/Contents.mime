{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int iteration = detail(-2, \"ivalue\");

int clip_len = detail(-1, \"numpts\");
int min_seg_len = chi(\"../minseglength_\" + itoa(iteration));
int sample_size = floor(chi(\"../samplesize_\" + itoa(iteration)));
int num_samples = (clip_len - min_seg_len) / sample_size;

num_samples = (clip_len) / sample_size;

int offset_sample_idx = @primnum % num_samples;
int len_sample_idx = floor( @primnum / num_samples) ;



i@offset_sample_idx = offset_sample_idx;
i@len_sample_idx = len_sample_idx;

i@trim_start = offset_sample_idx * sample_size + 1;
i@trim_end = (i@trim_start + (len_sample_idx *sample_size) + min_seg_len) + 1;
"	)
