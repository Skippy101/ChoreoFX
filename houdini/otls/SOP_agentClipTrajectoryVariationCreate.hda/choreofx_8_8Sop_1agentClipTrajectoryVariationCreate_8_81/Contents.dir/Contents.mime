Content-Type: text/plain

{
    channel npts {
      lefttype = extend
      righttype = extend
      default = 5000
      flags = 0
      segment { length = 0 value = 5000 5000 expr = "{
    float numpts = detail(-1, \"numpts\", 0);
    float searchseglength  = ch(\"../samplesize_\" + detail(-2, \"ivalue\", 0));
    float numsegs = floor(numpts / searchseglength)^2;
    return if(numsegs<1, 1, numsegs);
}" }
    }
  }

