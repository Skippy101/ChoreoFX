--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/pointgenerate2.chn"
Content-Type: text/plain

{
    channel npts {
      lefttype = extend
      righttype = extend
      default = 5000
      flags = 0
      segment { length = 0 value = 5000 5000 expr = "{
    float numpts = detail(-1, \"numpts\", 0);
    float searchseglength  = ch(\"../samplesize_\" + detail(-2, \"ivalue\", 0));
    float minseglength = ch(\"../minseglength_\" + detail(-2, \"ivalue\", 0));
    float trim_beginning = ch(\"../trim_beginning_\" + detail(-2, \"ivalue\", 0));
    float trim_end = ch(\"../trim_end_\" + detail(-2, \"ivalue\", 0));
    float numsegs = floor((numpts - 1 - minseglength) / searchseglength);
    return if((trim_beginning || trim_end), if((trim_beginning && trim_end), (numsegs*(numsegs+1))/2,numsegs), 1);

}" }
    }
  }

