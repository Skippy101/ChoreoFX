--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/set_start_and_end_frame_prim_attribs_timming.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int num_pts = detail(-1, \"numpts\");
int iteration = detail(-2, \"ivalue\");
int trim_end = chi(\"../trim_end_\" + itoa(iteration));
int trim_beginning = chi(\"../trim_beginning_\" + itoa(iteration));
int search_seg_len = chi(\"../samplesize_\" + itoa(iteration));
int min_seg_len = chi(\"../minseglength_\" + itoa(iteration));


int max_rows = floor((num_pts - 1 - min_seg_len) / search_seg_len) - 1;
if(trim_beginning && trim_end){
    // triangular number formula = n(n + 1) / 2 = i
    // to get the triangular number row of an int int((-1 + sqrt(1 + 8 * i)) / 2);
    
    int row =int((-1 + sqrt(1 + 8 * ( @primnum ))) / 2);
    int col = @primnum - (row *(row+1) / 2);
    i@trim_start = ((max_rows - row)  * search_seg_len) + 1;
    i@trim_end =num_pts - (col * search_seg_len) ;
}
else{
    if(trim_end){
        i@trim_end =1 + num_pts - (@primnum * search_seg_len);
        i@trim_start = 1;
    }
    else{
        i@trim_start = @primnum * search_seg_len + 1;
        i@trim_end = num_pts + 1;
    }
}
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
