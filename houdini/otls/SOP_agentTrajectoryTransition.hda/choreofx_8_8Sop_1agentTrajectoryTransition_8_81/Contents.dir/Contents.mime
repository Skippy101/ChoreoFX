snippet	[ 0	locks=0 ]	(	"

int transition_len = chi(\"../transition_len\");

int last_ptnum = detail(1, \"numpts\") - 1;

int ref_frame = last_ptnum ;

/*
vector4 last_orient = point(1, \"orient\", last_ptnum);
vector last_pos = point(1, \"P\", last_ptnum);
matrix last_xform = qconvert(last_orient, last_pos);
*/

vector4 ref_orient = point(1, \"orient\", ref_frame); 
vector ref_P = point(1, \"P\", ref_frame);
matrix ref_xform = qconvert(ref_orient, ref_P);

matrix start_xform_B = ref_xform;// slerp(ref_xform, last_xform, 0.5);

vector4 start_orient_A = point(0, \"orient\", transition_len);
vector start_pos_A = point(0, \"P\", transition_len);
matrix start_xform_A = qconvert(start_orient_A, start_pos_A);

matrix xform = invert(start_xform_A) * start_xform_B;

@P *= xform;
p@orient = qmultiply(quaternion(matrix3(xform)), p@orient);
"	)
