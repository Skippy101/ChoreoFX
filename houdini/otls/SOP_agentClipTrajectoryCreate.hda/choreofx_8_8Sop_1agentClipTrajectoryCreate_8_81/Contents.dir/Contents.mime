string geo_agentnames [] = uniquevals(0, \"point\", \"agentname\");

int pts_to_keep [] = array();

foreach(string agentname; geo_agentnames){
    if(match(target_agentnames, agentname)){
        append(pts_to_keep, findattribval(0, \"point\", \"agentname\", agentname, 0));
    }
}

if(find(pts_to_keep, @ptnum)<0){
    removepoint(0, @ptnum);
}
